<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name ="log4net" type="log4net.Config.Log4NetConfigutationSectionHandler, log4net" />
	</configSections>
	<appSettings>
		<add key="layout" value="%date %level %message%newline"/>
	</appSettings>
	<log4net>
		<appender name="FileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="log-file-winform.txt"/>
			<encoding value="utf-8"/>
			<appendToFile value="true" />
			<rollingStyle value="Once"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %level [%thread] %type.%method %message%newline%exception" />
			</layout>
		</appender>
		<root>
			<level value="ALL" />
			<appender-ref ref="FileAppender" />
		</root>
	</log4net>
	
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
</configuration>